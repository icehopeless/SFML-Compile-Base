all: compile link run

compile:
	g++ -IC:\SFML-2.5.1\include -c main4.cpp
	g++ -IC:\SFML-2.5.1\include -c main3.cpp
	g++ -IC:\SFML-2.5.1\include -c main2.cpp
	g++ -IC:\SFML-2.5.1\include -c main.cpp //included outhers mains

link:
	g++ main.o -o main -LC:\SFML-2.5.1\lib -lsfml-audio -lsfml-graphics -lsfml-window -lsfml-system
	
	
run:
	./main.exe
